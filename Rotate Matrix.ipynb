    "# solution with a new list\n",
    "def rotate_matrix(matrix,n):\n",
    "    matrix_after_rotation=[]\n",
    "    for i in range(n):\n",
    "        temp=[]\n",
    "        for j in range(n-1,-1,-1):\n",
    "            temp.append(matrix[j,i])\n",
    "        matrix_after_rotation.append(temp)\n",
    "    return matrix_after_rotation"
   ]
